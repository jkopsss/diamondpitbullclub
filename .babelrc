{
    "presets": [
      "next/babel"
      
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "module-resolver",
        {
          "alias": {
            "assets": "./src/assets", 
            "section": "./src/section",
            "common": "./src/common",
            "utils": "./utils/"
          }
        }
      ]
    ]
  }